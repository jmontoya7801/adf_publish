{
	"name": "ds_parquet_dir",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_datalake_dllo",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"Directory": {
				"type": "string"
			},
			"Filename": {
				"type": "string"
			},
			"Filesystem": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": ".parquet",
				"folderPath": {
					"value": "@concat(dataset().Directory, '/', dataset().Filename)",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().Filesystem",
					"type": "Expression"
				}
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}